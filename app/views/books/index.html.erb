<h1>
  <strong>Books</strong>
</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th></th>
    </tr>
    
  </thead>
  
  
</table>




<h1>
  <strong>New Book</strong>
</h1>

<%= form_with model: Book.new do |f| %>
  <h4>Tiile</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Book' %>
<% end %>